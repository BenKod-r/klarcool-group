{% extends 'base.html.twig' %}

{% block title %}Klarcool produits{% endblock %}

{% block body %}
    <main>
        <!--? Hero Start -->
        <div class="slider-area2">
            <div class="slider-height2 d-flex align-items-center">
                <div class="container">
                    <div class="row">
                        <div class="col-xl-12">
                            <div class="hero-cap hero-cap2 pt-70">
                                <h2>{% trans %}Nos Produits{% endtrans %}</h2>
                                <nav aria-label="breadcrumb">
                                    <ol class="breadcrumb">
                                        <li class="breadcrumb-item"><a href="{{ path('home_index') }}">{% trans %}Accueil{% endtrans %}</a></li>
                                        <li class="breadcrumb-item"><a href="{{ path('product') }}">{% trans %}Nos Produits{% endtrans %}</a></li>
                                    </ol>
                                </nav>
                                <a href="{{ path('contact') }}" class="btn btn-success circle mt-40">{% trans %}Contactez-nous{% endtrans %}</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Hero End -->
        <!--? Blog Area Start-->
        <section class="blog_area section-padding">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12 mb-5 mb-lg-0">
                        <div class="blog_left_sidebar">
                            <article class="blog_item">
                                <div class="blog_item_img">
                                    <img class="card-img rounded-0" src="{{ asset('assets/img/gallery/duraclim1bis.jpg') }}" alt="duraclim">
                                    <a href="{{ path('product_duraclim') }}" class="blog_item_date">
                                        <h3>DURACLIM</h3>
                                        <p>{% trans %}Visitez{% endtrans %}</p>
                                    </a>
                                </div>
                                <div class="blog_details">
                                    <a class="d-inline-block" href="{{ path('product_duraclim') }}">
                                        <h2 class="blog-head" style="color: #2d2d2d;">{% trans %}Activités & Métiers de Klarcool{% endtrans %}</h2>
                                    </a>
                                    <ul class="mb-5 ml-3">
                                        <li style="list-style-type: circle;">{% trans %}Fabrication de machines automatisées de recharge de climatisation  automobile utilisant le gaz écologique Duracool{% endtrans %}</li>
                                        <li style="list-style-type: circle;">{% trans %}Fabrication de machines de nettoyage des moteurs par hydrogène marque Motorklar sans sous-traitance{% endtrans %}</li>
                                        <li style="list-style-type: circle;">{% trans %}Fabrication de pompes à chaleur 12 à 30 kw 100% écologique utilisant le gaz Duracool{% endtrans %}</li>
                                        <li style="list-style-type: circle;">{% trans %}Ingénierie bureau d'étude et développement de nouveaux systèmes écologiques liées à l'automobile et l'habitat{% endtrans %}</li>
                                        <li style="list-style-type: circle;">{% trans %}Formation sur la technique la gamme machine de la société{% endtrans %}</li>
                                    </ul>
                                    <ul class="blog-info-link">
                                        <li><a href="{{ asset('assets/pdf/201001-book-presentation-v.06 fmi.pdf') }}" download><i class="far fa-file-pdf"></i> Documentation</a></li>
                                        <li><a href="{{ path('contact') }}"><i class="far fa-envelope-open"></i> Contact</a></li>
                                    </ul>
                                </div>
                            </article>
                            <article class="blog_item">
